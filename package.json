{
  "name": "react-common-state-hooks",
  "version": "0.0.8",
  "description": "A set of wrappers around React's useState hook for many common scenarios",
  "main": "index.js",
  "types": "./index.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "webpack",
    "prepare": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matt-hernandez/react-common-state-hooks.git"
  },
  "keywords": [
    "react",
    "hooks",
    "state",
    "common",
    "useState"
  ],
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "author": "Matt Hernandez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matt-hernandez/react-common-state-hooks/issues"
  },
  "homepage": "https://github.com/matt-hernandez/react-common-state-hooks#readme",
  "dependencies": {
    "@types/react": "^16.9.0",
    "react": "^16.9.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^25.1.1",
    "coveralls": "^3.0.9",
    "jest": "^25.1.0",
    "react-test-renderer": "^16.12.0",
    "ts-jest": "^25.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "3.5.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
