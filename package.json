{
  "name": "react-common-state-hooks",
  "version": "1.1.10",
  "description": "A set of wrappers around React's useState hook for many common scenarios",
  "main": "index.js",
  "types": "./index.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prebuild": "npm test",
    "build": "webpack",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matt-hernandez/react-common-state-hooks.git"
  },
  "keywords": [
    "react",
    "hooks",
    "state",
    "common",
    "useState"
  ],
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "author": "Matt Hernandez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matt-hernandez/react-common-state-hooks/issues"
  },
  "homepage": "https://github.com/matt-hernandez/react-common-state-hooks#readme",
  "dependencies": {
    "@types/react": "^17.0.39",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0",
    "coveralls": "^3.1.1",
    "jest": "^27.5.1",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  }
}
